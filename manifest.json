{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon_16.png"
   },
   "description": "ページ内のTweetButtonを使ってツイートします。いちいちボタンを探す必要はありません。",
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png"
   },
   "manifest_version": 2,
   "name": "ExTweetButton",
   "permissions": [
      "tabs",
      "http://*/",
      "https://*/"
    ],
   "version": "1.0"
}
